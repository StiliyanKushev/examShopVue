AAA,QADA;AAEA,QAAA,SAAA,EAAA,SAAA,CAAA,CAAA;AAFA;AALA;AADA,GAXA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,YADA,0BACA;AAAA;;AACA,WAAA,EAAA,CAAA,IAAA,CAAA,MAAA;;AAEA,UAAA,KAAA,EAAA,CAAA,IAAA,CAAA,QAAA,EAAA;AACA;AACA,YAAA,MAAA,GAAA,EAAA;AAEA,YAAA,CAAA,KAAA,EAAA,CAAA,IAAA,CAAA,KAAA,CAAA,QAAA,EAAA,MAAA,CAAA,IAAA,CAAA,oBAAA;AACA,YAAA,CAAA,KAAA,EAAA,CAAA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA,MAAA,CAAA,IAAA,CAAA,mBAAA;AACA,YAAA,CAAA,KAAA,EAAA,CAAA,IAAA,CAAA,QAAA,CAAA,QAAA,EAAA,MAAA,CAAA,IAAA,CAAA,uBAAA;AACA,YAAA,CAAA,KAAA,EAAA,CAAA,IAAA,CAAA,QAAA,CAAA,SAAA,EAAA,MAAA,CAAA,IAAA,CAAA,yCAAA;;AAEA,mCAAA,MAAA;AAAA,cAAA,GAAA,cAAA;AAAA,eAAA,UAAA,CAAA,KAAA,CAAA;AAAA,YAAA,KAAA,EAAA,GAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AAAA;AACA,OAVA,MAWA;AACA,aAAA,KAAA,CAAA,IAAA,CAAA,kCAAA,EAAA,KAAA,IAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,cAAA,CAAA,GAAA,CAAA,IAAA,CAAA,OAAA,EAAA;AACA,YAAA,KAAA,CAAA,UAAA,CAAA,KAAA,CAAA;AAAA,cAAA,KAAA,EAAA,GAAA;AAAA,cAAA,IAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAAA,aAAA;AACA,WAFA,MAGA;AACA,YAAA,KAAA,CAAA,UAAA,CAAA,OAAA,CAAA;AAAA,cAAA,KAAA,EAAA,GAAA;AAAA,cAAA,IAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AAAA,aAAA;;AACA,YAAA,WAAA,CAAA,QAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,QAAA,EAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;;AACA,YAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA,EAHA,CAGA;;AACA;AACA,SAVA;AAWA;AACA;AA5BA;AAvBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"form\">\r\n                <form @submit.prevent=\"handleSubmit\">\r\n                    <div class=\"container\">\r\n                        <label for=\"email\">\r\n                            Email\r\n                        </label>\r\n                        <input v-model=\"$v.form.email.$model\" type=\"text\" name=\"email\" placeholder=\"Enter Email\" />\r\n                        <label for=\"password\">\r\n                            Password\r\n                        </label>\r\n                        <input v-model=\"$v.form.password.$model\"  type=\"password\" name=\"password\" placeholder=\"Enter Password\" />\r\n                        <button type=\"submit\" class=\"SubmitButton\">Login</button>\r\n                    </div>\r\n                    <div class=\"otherOption\">\r\n                        <p>Don't have an account? <router-link to=\"/register\">Register.</router-link></p>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n</template>\r\n\r\n<script>\r\nimport userHandler from '../../handlers/user';\r\nimport { validationMixin } from 'vuelidate';\r\nimport { minLength, email, required } from 'vuelidate/lib/validators';\r\nexport default {\r\n    mixins:[validationMixin],\r\n    name: 'LoginView',\r\n    data(){\r\n        return {\r\n            form:{\r\n                email:'',\r\n                password:'',\r\n            }\r\n        }\r\n    },\r\n    validations:{\r\n        form:{\r\n            email:{\r\n                required,\r\n                email\r\n            },\r\n            password:{\r\n                required,\r\n                minLength: minLength(8)\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        handleSubmit(){\r\n            this.$v.form.$touch();\r\n\r\n            if(this.$v.form.$invalid){\r\n                //vue notify errors\r\n                let errors = [];\r\n\r\n                if(!this.$v.form.email.required) errors.push(\"Email is required!\");\r\n                if(!this.$v.form.email.email) errors.push(\"Email is invalid!\");\r\n                if(!this.$v.form.password.required) errors.push(\"Password is required!\");\r\n                if(!this.$v.form.password.minLength) errors.push(\"Password must be at least 8 chars long!\");\r\n\r\n                for(let err of errors) this.$vToastify.error({title:\" \", body: err });\r\n            }\r\n            else{\r\n                this.$http.post(\"http://localhost:9999/auth/login\",this.form)\r\n                .then((res) => {\r\n                    if(!res.data.success){\r\n                        this.$vToastify.error({title:\" \", body: res.data.message });\r\n                    }\r\n                    else{\r\n                        this.$vToastify.success({title:\" \", body: res.data.message });\r\n                        userHandler.saveUser(res.data.token,res.data.user.username,res.data.user.roles);\r\n                        this.$router.push('/');// redirect to home\r\n                    }\r\n                });\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>"],"sourceRoot":"src/components/views"}]}